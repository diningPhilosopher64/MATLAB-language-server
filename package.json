{
  "name": "matlab-language-server",
  "version": "1.2.2",
  "description": "Language Server for MATLAB code",
  "main": "./src/index.ts",
  "bin": "./out/index.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode development && cd src/licensing/gui && npm run build && cd ../../.. && cp -R src/licensing/gui/build/* ./out/licensing/gui/build/ && rm -rf ./out/licensing/gui/src",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "url": "https://github.com/mathworks/MATLAB-language-server"
  },
  "author": "The MathWorks, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "node-loader": "^2.0.0",
    "nodemon": "^3.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node-fetch": "^2.6.11",
    "@types/xml2js": "^0.4.14",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "faye": "^1.4.0",
    "node-fetch": "^3.3.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.7",
    "vscode-uri": "^3.0.6",
    "xml2js": "^0.6.2",
    "yargs": "^17.5.1"
  }
}
